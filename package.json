{
  "name": "hiynn-design",
  "version": "0.3.37",
  "description": "base on react and antd layout framework",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "homepage": "http://hiynn.design",
  "scripts": {
    "create:cssjs": "rimraf lib es && cd scripts && gulp cssjs",
    "build:cssjs": "cross-env BABEL_ENV=commonjs babel lib/**/style -d lib/**/style --source-maps",
    "build:css": "cd scripts && gulp compile",
    "build:umd": "cross-env BABEL_ENV=umd webpack --config scripts/webpack.config.umd.js",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel components -d lib --source-maps",
    "build:es": "cross-env BABEL_ENV=es babel components -d es --source-maps",
    "build:all": "yarn build:umd && yarn create:cssjs && yarn build:cssjs && yarn build:commonjs && yarn build:es && yarn build:css",
    "build:prod": "cross-env BABEL_ENV=preview webpack --config scripts/webpack.config.prod.js",
    "start": "cross-env BABEL_ENV=preview webpack-dev-server --port 7006 --open --config scripts/webpack.config.dev.js",
    "deploy:docs": "yarn build:prod && gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiynn-com/hiynn-design.git"
  },
  "keywords": [
    "react",
    "antd",
    "layout"
  ],
  "author": "zhangwei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiynn-com/hiynn-design/issues"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
